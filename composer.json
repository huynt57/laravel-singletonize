{
    "name": "codex/laravel-singletonize",
    "description": "Laravel package to default container resolution to singleton",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/container": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codex\\LaravelSingletonize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codex\\LaravelSingletonize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codex\\LaravelSingletonize\\SingletonizeServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "php tests/run.php"
    }
}
